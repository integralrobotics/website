    "cssnano": "^7.0.6",
    "eleventy-plugin-svg-sprite": "^2.4.2",
    "modern-normalize": "^3.0.1",
    "postcss": "^8.5.1",
    "postcss-cli": "^11.0.0",
    "postcss-custom-media": "^11.0.5",
